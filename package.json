{
  "name": "quiz-builder-frontend",
  "version": "1.0.0",
  "description": "Quiz-builder front-end",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "create": "webpack",
    "start": "webpack-dev-server --open",
    "test": "jest",
    "coverage": "NODE_ENV=test jest --coverage --coverageReporters=lcov",
    "localcoverage": "jest --coverage --coverageReporters=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estand25/quiz-builder-frontend.git"
  },
  "keywords": [],
  "author": "Standley Eugene",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/estand25/quiz-builder-frontend/issues"
  },
  "homepage": "https://github.com/estand25/quiz-builder-frontend#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.4.1",
    "local-storage": "^2.0.0",
    "lodash": "^4.17.21",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@testing-library/react": "^9.4.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "coveralls": "^3.0.9",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^24.9.0",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^1.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/public/",
      "<rootDir>/index.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.{js}",
      "!src/**/__tests__/*.{js}"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  }
}
